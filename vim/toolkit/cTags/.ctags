--langdef=ini
--langmap=ini:.ini
--regex-ini=/^[ \t]*\[(.*)\]/\1/b,block/
--regex-ini=/^[ \t]*([=]+)=(.*)/\1/k,key/
--langdef=uc
--langmap=uc:.uc
--regex-uc=/^[ \t]*[cC]lass[ \t]+([a-zA-Z0-9_]+)/\1/c,class/
--regex-uc=/^[ \t]*[sS]tate[() \t]+([a-zA-Z0-9]+)/\1/s,state/
--regex-uc=/^[ \t]*[sS]imulated[ \t]*(.*)[ \t]+([a-zA-Z0-9_]+)\(/\2/i,simulated/
--regex-uc=/^[ \t]*[eE]vent[ \t]*[^ \t]*[ \t]+([a-zA-Z0-9_]+)\(/\1/e,event/
--regex-uc=/^[ \t]*[fF]unction[ \t]*(.*)[ \t]+([a-zA-Z0-9_]+)\(/\2/f,function/
--regex-uc=/^[ \t]*[eE]xec[ \t]*(.*)[ \t]+([a-zA-Z0-9_]+)\(/\2/x,exec/
--regex-uc=/^[ \t]*[sS]truct[ \t]*(.*)[ \t]+([a-zA-Z0-9_]+)/\2/t,struct/
--regex-uc=/^[ \t]*[eE]num[ \t]*(.*)[ \t]+([a-zA-Z0-9_]+)/\2/m,enum/
--langdef=maxscript
--langmap=maxscript:.ms
--regex-maxscript=/^[ \t]*fn[ \t]+([a-zA-Z0-9_]+)/\1/f,function/
--regex-maxscript=/^[ \t]*structure[ \t]+([a-zA-Z0-9_]+)/\1/c,class/
--regex-maxscript=/^[ \t]*rollout[ \t]+([a-zA-Z0-9_]+)/\1/c,class/
--regex-maxscript=/^[ \t]*checkbox[ \t]+([a-zA-Z0-9_]+)/\1/c,class/
--regex-maxscript=/^[ \t]*button[ \t]+([a-zA-Z0-9_]+)/\1/c,class/
--regex-maxscript=/^[ \t]*label[ \t]+([a-zA-Z0-9_]+)/\1/c,class/
--regex-maxscript=/^[ \t]*global[ \t]+([a-zA-Z0-9_]+)/\1/c,class/
--regex-maxscript=/^[ \t]*utility[ \t]+([a-zA-Z0-9_]+)/\1/c,class/
--regex-maxscript=/^[ \t]*group[ \t]+([a-zA-Z0-9_]+)/\1/c,class/
